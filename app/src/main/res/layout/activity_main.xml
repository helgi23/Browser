<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">

    <AutoCompleteTextView
            android:layout_width="190dp"
            android:layout_height="40dp"
            android:id="@+id/autoCompleteTextView" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.717"
            android:backgroundTint="#000000" app:layout_constraintBottom_toTopOf="@+id/webView"/>
    <ProgressBar
            style="?android:attr/progressBarStyle"
            android:layout_width="33dp"
            android:layout_height="40dp"
            android:id="@+id/progressBar"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/webView" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.839"
            app:layout_constraintVertical_bias="0.031"/>
    <WebView
            android:layout_width="385dp"
            android:layout_height="617dp"
            android:id="@+id/webView" android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.6"
            app:layout_constraintVertical_bias="0.818">
    </WebView>
    <ImageButton
            android:layout_width="40dp"
            android:layout_height="45dp" app:srcCompat="@drawable/baseline_fast_rewind_black_18dp"
            android:id="@+id/backButton" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginBottom="8dp"
            app:layout_constraintHorizontal_bias="0.022" app:layout_constraintVertical_bias="0.592"
            app:layout_constraintBottom_toTopOf="@+id/webView"/>
    <ImageButton
            android:layout_width="40dp"
            android:layout_height="45dp" app:srcCompat="@drawable/baseline_fast_forward_black_18dp"
            android:id="@+id/forwardButton" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.135"
            app:layout_constraintVertical_bias="0.592" app:layout_constraintBottom_toTopOf="@+id/webView"/>
    <ImageButton
            android:layout_width="40dp"
            android:layout_height="45dp" app:srcCompat="@drawable/baseline_autorenew_black_18dp"
            android:id="@+id/refreshButton"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintHorizontal_bias="0.267"
            app:layout_constraintVertical_bias="0.592" app:layout_constraintBottom_toTopOf="@+id/webView"/>
    <ImageButton
            android:layout_width="40dp"
            android:layout_height="45dp" app:srcCompat="@drawable/baseline_play_arrow_black_18dp"
            android:id="@+id/sendButton" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginBottom="8dp"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.977"
            app:layout_constraintVertical_bias="0.592" app:layout_constraintBottom_toTopOf="@+id/webView"/>

</android.support.constraint.ConstraintLayout>